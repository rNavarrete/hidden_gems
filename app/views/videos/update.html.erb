<h1>Update your video</h1>

<div class="row">
  <div class="span6 offset3">
    <%= simple_form_for(@video) do |f| %>
    <%= f.input  :video_link %>
    <%= f.input  :artist %>
    <%= f.input  :song %>
    <%= f.input  :description, as: :text %>      
    <%= f.button :submit, class: "btn btn-large btn-primary" %>
  <% end %>
  </div>
</div>